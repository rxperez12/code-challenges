{
  "name": "code-challenges",
  "version": "1.0.0",
  "description": "code challenges for interview prep",
  "main": "index.js",
  "scripts": {
    "test": "vitest --exclude '**/*.js'",
    "test:cov": "vitest --exclude '**/*.js' --coverage --coverage.exclude=\"**/*start*\" --coverage.exclude=\"**/common\" --coverage.reportOnFailure",
    "test:debug": "vitest --exclude '**/*.js' --run --inspectBrk --no-file-parallelism",
    "test:ui": "vitest --exclude '**/*.js' --ui"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "vitest": "^1.3.1"
  }
}
